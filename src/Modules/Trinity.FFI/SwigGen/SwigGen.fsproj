<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyName>SwigGen</AssemblyName>
    <RootNamespace>SwigGen</RootNamespace>
    <OutputTypeEx>library</OutputTypeEx>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <OutputPath>..\..\..\..\bin</OutputPath>
    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GraphEngine.Core" Version="2.0.9328" />
    <PackageReference Include="GraphEngine.Jit" Version="2.0.9328" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="AssemblyInfo.fs" />
    <Compile Include="Lib.fs" />
    <Content Include="packages.config" />
    <Compile Include="Gen.fs" />
  </ItemGroup>
</Project>
