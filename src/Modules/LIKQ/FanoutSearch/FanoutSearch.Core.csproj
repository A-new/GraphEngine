  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Properties\AssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\..\BuildProfile\ProductInfo.cs" Link="ProductInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <TslCodeGen Include="TSL\Core.tsl" />
