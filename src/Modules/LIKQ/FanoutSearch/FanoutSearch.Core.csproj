<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Version>8.0.30703</Version>
    <AssemblyVersion>8.0.30703.0</AssemblyVersion>
