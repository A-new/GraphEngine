<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Version>8.0.30703</Version>
    <AssemblyVersion>8.0.30703.0</AssemblyVersion>
    <FileVersion>8.0.30703.0</FileVersion>
    <RootNamespace>FanoutSearch</RootNamespace>
    <AssemblyName>FanoutSearch.CoreCLR</AssemblyName>
    <Authors>Microsoft Graph Engine Team</Authors>
    <Company>Microsoft Corporation</Company>
    <Copyright>Copyright 2017</Copyright>
    <PackageLicenseUrl>https://github.com/Microsoft/GraphEngine/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>https://www.graphengine.io</PackageProjectUrl>
    <PackageIconUrl>https://www.graphengine.io/img/logo-64x64.png</PackageIconUrl>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <OutputPath>..\..\..\..\bin\coreclr\</OutputPath>
  </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <OutputPath>..\..\..\..\bin\coreclr\</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Properties\AssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\..\BuildProfile\ProductInfo.cs" Link="ProductInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <TslCodeGen Include="TSL\Core.tsl" />
    <TslCodeGen Include="TSL\Module.tsl" />
    <TslCodeGen Include="TSL\Result.tsl" />
    <TslCodeGen Include="TSL\Web.tsl" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="GraphEngine.CoreCLR" Version="1.0.8850" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="2.6.1" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Scripting" Version="2.6.1" />
    <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="2.0.0" />
    <PackageReference Include="Serialize.Linq" Version="1.4.0" />
  </ItemGroup>
</Project>
